    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PROCESS" minOccurs="0" />
        <xs:element ref="REACTANT_ID" minOccurs="0" maxOccurs="unbounded" />
        <xs:element ref="CATALYST_ID" minOccurs="0" maxOccurs="unbounded" />
